<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="keywords" content="Organic" />
    <script type="text/javascript">
    addEventListener("load", function () {
      setTimeout(hideURLbar, 0);
    }, false);


    function hideURLbar() {
      window.scrollTo(0, 1);
    }
    </script>
    <!--booststrap-->
    <link href="../Content/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all"/>
    <!--//booststrap end-->
    <!-- font-awesome icons -->
    <link href="../Content/css/font-awesome.min.css" rel="stylesheet"/>
    <!-- //font-awesome icons -->
    <!--stylesheets-->
    <link href="../Content/css/style.css" rel='stylesheet' type='text/css' media="all"/>
    <!--//stylesheets-->
    <link href="//fonts.googleapis.com/css?family=Raleway:400,500,600,700" rel="stylesheet"/>
    <link href="//fonts.googleapis.com/css?family=Patrick+Hand" rel="stylesheet"/>
    <link href="//fonts.googleapis.com/css?family=Roboto:400,500,700" rel="stylesheet"/>

</head>
<body>


    <div class="banner-left-side" id="home">
        <!-- header -->
        <div class="headder-top">
            <!-- nav -->
            <nav>
                <div id="logo">
                    <h1>
                        <a href="#">OS</a>
                    </h1>
                </div>
                <div class="sub-headder position-relative">
                    <h6>
                        <a href="#">
                            Organic
                            <br>Store
                        </a>
                    </h6>
                </div>
                <label for="drop" class="toggle">Menu</label>
                <input type="checkbox" id="drop">
                <ul class="menu mt-2">
                    <li class="active">
                        <a>@Html.ActionLink("Home", "Index", "Home")</a>
                    </li>
                    <li>
                        <a>@Html.ActionLink("About", "About", "Home")</a>
                    </li>
                    <li>
                        <a>@Html.ActionLink("Services", "Services", "Home")</a>
                    </li>
                    <li>

                    <li>
                        <a>@Html.ActionLink("Gallery", "Gallery", "Home")</a>
                    </li>

                    <li>
                        <a>@Html.ActionLink("Contact", "Contact", "Home")</a>
                    </li>
                    <li>
                        <a>@Html.ActionLink("Registration", "Registration", "Home")</a>
                    </li>
                    <li>
                        <a>@Html.ActionLink("Login", "Login", "Home")</a>
                    </li>
                </ul>
            </nav>
            <!-- //nav -->
        </div>
        <!-- //header -->
        <!-- banner -->
        <div class="main-banner text-center">
            <div class="container">
                <div class="social-icons mb-lg-4 mb-3">
                    <ul>
                        <li class="facebook">
                            <a href="#">
                                <span class="fa fa-facebook"></span>
                            </a>
                        </li>
                        <li class="twitter">
                            <a href="#">
                                <span class="fa fa-twitter"></span>
                            </a>
                        </li>
                        <li class="rss">
                            <a href="#">
                                <span class="fa fa-rss"></span>
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="banner-right-txt">
                    <h5 class="mb-sm-3 mb-2">Healthy Food</h5>
                    <h4>Organic Store </h4>
                </div>
                <div class="slide-info-txt">
                    <p>
                        
                    </p>
                </div>
            </div>
        </div>
    </div>

    <div>
        @RenderBody()
    </div>


    <section class="footer-w3layouts-bottem py-lg-4 py-md-3 py-sm-3 py-3">
        <div class="container py-lg-5 py-md-5 py-sm-4 py-3">
            <div class="row ">
                <div class="footer-bottom-info col-lg-4 col-md-4 ">
                    <h4 class="pb-lg-4 pb-md-3 pb-3 ">Address</h4>
                    <div class="bottom-para ">
                        <div class="footer-office-hour">
                            <ul>
                                <li class="mb-2">
                                    <h6>Address</h6>
                                </li>
                                <li>
                                    <p>
                                        Sector 5, Ahemadabad City,
                                        <br>INDIA.
                                    </p>
                                </li>
                            </ul>
                            <ul>
                                <li class="my-2">
                                    <h6>Phone</h6>
                                </li>
                                <li>
                                    <p>+ 91 9998887776</p>
                                </li>
                                <li class="my-2">
                                    <h6>Email</h6>
                                </li>
                                <li>
                                    <p>
                                        <a href="#">orgstor@gmail.com</a>
                                    </p>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="footer-bottom-info col-lg-4 col-md-4 ">
                    <h4 class="pb-lg-4 pb-md-3 pb-3 ">Twitter Us</h4>
                    <div class="footer-office-hour">
                        <ul>
                            <li>
                                <p></p>
                            </li>
                            <li class="my-1">
                                <p>
                                    <a href="#">healthstore@gmail.com</a>
                                </p>
                            </li>
                            <li class="mb-3">
                                <span class="font-italic">Posted 3 days ago.</span>
                            </li>
                            <li>
                                <p></p>
                            </li>
                            <li class="my-1">
                                <p>
                                    <a href="#">organic@gmail.com</a>
                                </p>
                            </li>
                            <li>
                                <span class="font-italic">Posted 3 days ago.</span>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="footer-bottom-info col-lg-4 col-md-4 ">
                    <h4 class="pb-lg-4 pb-md-3 pb-3 ">NewsLetter</h4>
                    <div class="newsletter">
                        <form action="#" method="post" class="d-flex">
                            <input type="email" name="Your Email" class="form-control" placeholder="Your Email" required="">
                            <button class="btn1">
                                <span class="fa fa-envelope-o" aria-hidden="true"></span>
                            </button>
                        </form>
                    </div>
                    <div class="footer-office-hour mt-3">
                        <p></p>
                    </div>
                </div>
            </div>
            <!-- move icon -->
            <div class="text-center mt-lg-5 mt-md-4 mt-3">
                <a href="#home" class="move-top text-center mt-3">
                    <span class="fa fa-arrow-up" aria-hidden="true"></span>
                </a>
            </div>
            <!--//move icon -->
        </div>
    </section>
    <!--footer-copy-right -->
    <footer class="bottem-wthree-footer text-center py-md-4 py-3">
        <p>
            ©2023 Organic Store. All Rights Reserved | Developed by
            <a href="#" target="_blank">Jayesh Ahir</a>
        </p>
    </footer>
    <!--//footer-copy-right -->


</body>
</html>
